{
  "name": "omni",
  "private": true,
  "workspaces": [
    "backend",
    "landing-page/frontend",
    "frontend/customer-frontend",
    "frontend/broker-frontend",
    "frontend/worker-frontend"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace backend",
    "dev:landing:frontend": "npm run dev --workspace landing-page/frontend",
    "dev:customer": "npm run dev --workspace frontend/customer-frontend",
    "dev:broker": "npm run dev --workspace frontend/broker-frontend",
    "dev:worker": "npm run dev --workspace frontend/worker-frontend",
    "build": "npm run build --workspaces --if-present"
  }
}
